{
  "routes": [
    { "src": "/.*", "dest": "/index.html" }
  ]
}

{
  "routes": [
    { "src": "/.*", "dest": "/index.html" }
  ],
  "headers": [
    {
      "source": "**/*.(js|css|svg|png|jpg|jpeg|webp)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}


